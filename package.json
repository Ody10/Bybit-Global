{
  "name": "bybit-global",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:backend": "next dev -p 3001",
    "build": "prisma generate && prisma db push --accept-data-loss && next build",
    "build:backend": "prisma generate && next build",
    "build:mobile": "cross-env BUILD_MODE=mobile next build",
    "start": "next start",
    "start:backend": "next start -p 3001",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "db:reset": "prisma migrate reset && npm run db:seed",
    "mobile:build": "cross-env BUILD_MODE=mobile next build",
    "mobile:sync": "npm run mobile:build && npx cap sync",
    "mobile:android": "npm run mobile:sync && npx cap open android",
    "mobile:ios": "npm run mobile:sync && npx cap open ios",
    "android": "npx cap open android",
    "ios": "npx cap open ios",
    "cap:sync": "npx cap sync",
    "deploy:backend": "vercel --prod",
    "deploy:preview": "vercel"
  },
  "dependencies": {
    "@capacitor/android": "^6.1.2",
    "@capacitor/app": "^6.0.1",
    "@capacitor/core": "^6.2.1",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/splash-screen": "^6.0.2",
    "@capacitor/status-bar": "^6.0.1",
    "@prisma/client": "5.22.0",
    "bcryptjs": "2.4.3",
    "dotenv": "^17.2.3",
    "glob": "^13.0.0",
    "inflight": "^1.0.6",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "0.468.0",
    "next": "^16.1.1",
    "next-intl": "^4.7.0",
    "nodemailer": "6.9.16",
    "prisma": "5.22.0",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rimraf": "^6.1.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.2.1",
    "@react-native-community/cli": "^20.1.0",
    "@types/bcryptjs": "2.4.6",
    "@types/jsonwebtoken": "9.0.7",
    "@types/node": "20.17.10",
    "@types/nodemailer": "6.4.17",
    "@types/react": "18.3.14",
    "@types/react-dom": "18.3.2",
    "autoprefixer": "^10.4.23",
    "cross-env": "^7.0.3",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "ts-node": "^10.9.2",
    "typescript": "5.7.2"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
